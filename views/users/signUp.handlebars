<section class="h-100 flex-fill gradient-custom-2">
    <div class="container py-5 h-100">
      <div class="row d-flex justify-content-center align-items-center h-100">
        <div class="col">
          <div class="col-9 card card-registration ml-auto mr-auto">
            <div class="row ">

              
              <div class="col-xl-12">
                <br>
                <br>
                <div class="text-center">
                        <img src="../../public/img/logo1-removebg-preview.png" style="width: 260px;" alt="logo">
                          <br>
                        <h1 class="mt-1 mb-5 pb-1">SIGN UP FORM</h1>
                      </div>
                  <form method="POST" id="signup-form" onsubmit="signUpUI(event)">
                    <div class="container">
                    <div class="picture-container">
                        <div class="picture">
                          
                            <img src="https://lh3.googleusercontent.com/LfmMVU71g-HKXTCP_QWlDOemmWg4Dn1rJjxeEsZKMNaQprgunDTtEuzmcwUBgupKQVTuP0vczT9bH32ywaF7h68mF-osUSBAeM6MxyhvJhG6HKZMTYjgEv3WkWCfLB7czfODidNQPdja99HMb4qhCY1uFS8X0OQOVGeuhdHy8ln7eyr-6MnkCcy64wl6S_S6ep9j7aJIIopZ9wxk7Iqm-gFjmBtg6KJVkBD0IA6BnS-XlIVpbqL5LYi62elCrbDgiaD6Oe8uluucbYeL1i9kgr4c1b_NBSNe6zFwj7vrju4Zdbax-GPHmiuirf2h86eKdRl7A5h8PXGrCDNIYMID-J7_KuHKqaM-I7W5yI00QDpG9x5q5xOQMgCy1bbu3St1paqt9KHrvNS_SCx-QJgBTOIWW6T0DHVlvV_9YF5UZpN7aV5a79xvN1Gdrc7spvSs82v6gta8AJHCgzNSWQw5QUR8EN_-cTPF6S-vifLa2KtRdRAV7q-CQvhMrbBCaEYY73bQcPZFd9XE7HIbHXwXYA=s200-no" class="picture-src" id="displayPictureImg" title="upload a image" alt="no  " onerror="this.onerror=null; this.src='../../public/js/default_user.png'">
                            <input type="file" id="displayPicture" class="" required="">
                            <label for="displayPicture"></label>
                        </div>
                         <h2 class="fs-5">Choose Picture</h2>
                
                    </div>
                </div>
                <br>
                <br>
                  <div class="row">
                    <div class="col-md-6 mb-4">
                      <div class="form-outline">
                        <label class="form-label" for="signup-firstName" align="center">First name</label>
                        <input type="text" name="signup-firstName" class="form-control form-control-lg" id="signup-firstName" placeholder="Enter firstName" value="" required="">
                        
                      </div>
                    </div>
                    <div class="col-md-6 mb-4">
                      <div class="form-outline">
                        <label class="form-label" for="signup-lastName">Last name</label>
                        <input type="text" id="signup-lastName" class="form-control form-control-lg" name="signup-lastName" placeholder="Enter lastName" value="" required="">
                        
                      </div>
                    </div>
                  </div>
  
                  <div class="form-outline mb-4">
                    <label class="form-label" for="username">Username</label>
                    <input type="email" id="signup-username" class="form-control form-control-lg" name="signup-username" placeholder="Enter username" value="" required="" pattern="^[a-z0-9._%+-]+@[a-z0-9.-]+\.edu$" title="Please enter an edu mail">
                   
                  </div>

                  <div class="form-outline mb-4">
                    <label class="form-label" for="password">Password</label>
                    <input type="password" class="form-control form-control-lg" name="signup-password" id="signup-password" placeholder="Enter password" value="" required="" pattern="^[a-zA-Z0-9@.!_].{5,20}$" title="Please include one uppercase, one  lowercase, one special character, one number and min length of 5 and max length of 20">
                   
                  </div>
                  <div class="d-md-flex justify-content-start align-items-center mb-4 py-2">
  
                    <h3 class="mb-0 me-4 fs-5">Gender: </h3>
  
                    <div class="form-check form-check-inline mb-0 me-4">
                      <input class="form-check-input" type="radio" name="gender" id="femaleGender" value="option1">
                      <label class="form-check-label" for="femaleGender">Female</label>
                    </div>
  
                    <div class="form-check form-check-inline mb-0 me-4">
                      <input class="form-check-input" type="radio" name="gender" id="maleGender" value="option2">
                      <label class="form-check-label" for="maleGender">Male</label>
                    </div>
  
                    <div class="form-check form-check-inline mb-0">
                      <input class="form-check-input" type="radio" name="gender" id="otherGender" value="option3">
                      <label class="form-check-label" for="otherGender">Other</label>
                    </div>
  
                  </div>
                  <div class="form-outline mb-4">
                    <label class="form-label" for="age">Age</label>
                    <input type="number" id="age" class="form-control form-control-lg" pattern="^[a-zA-Z0-9@.!_].{5,20}$" placeholder=" Enter your Age" title="Enter your age" required="">
                  
                    
                  </div>
                  <div class="form-outline mb-4">
                    <label class="form-label" for="interests">Interests</label>
                    <input type="text" id="interests" class="form-control form-control-lg" rows="4" cols="30" pattern="^[a-zA-Z0-9@.!_].{5,20}$" placeholder="Please enter your intrests as comma seperated values"title="Please enter your interests as comma seperated values" required="">
                    <!-- <textarea id="interests" name="interests" rows="4" cols="30" required>
                    </textarea> -->
                    
                  </div>
                
                  <div class="form-outline mb-4">
                    <label class="form-label" for="description">Description</label>
                    <input type="text" id="description" class="form-control form-control-lg" pattern="^[a-zA-Z0-9@.!_].{5,20}$" placeholder="Enter Description" title="Please write a description about yourself" required="">
                    <!-- <textarea id="interests" name="interests" rows="4" cols="30" required>
                    </textarea> -->
                    
                  </div>

  
                  
  
                  <div class="form-outline mb-4">
                    <label class="form-label" for="genders">Genders Intrested In</label>
                    <input type="text" id="genders" class="form-control form-control-lg" pattern="^[a-zA-Z0-9@.!_].{5,20}$" placeholder="Please enter the genders in comma seperated values" title="Please enter the genders you are intrested in " required="">
                    
                  </div>
  
                  <div class="form-outline mb-4">
                    <label class="form-label" for="minAge">MinimumAge</label>
                    <input type="number" id="minAge" class="form-control form-control-lg" pattern="^[a-zA-Z0-9@.!_].{5,20}$" placeholder="Enter minimum age required" title="Please enter the Minimum age to match " required="">
                    
                  </div>
  
                  <div class="form-outline mb-4">
                    <label class="form-label" for="maxAge">Maximum Age</label>
                    <input type="number" id="maxAge" class="form-control form-control-lg" pattern="^[a-zA-Z0-9@.!_].{5,20}$" placeholder="Enter maximum age required" title="Please enter the Maximum age to match " required="">
                
                  </div>
    
              <div class="d-flex justify-content-center pt-6">
                  <a href="/users/login">Already have an account? Click here to log-in</a>
                  </div>
  
                  <div class="d-flex justify-content-center pt-3">
                    <button type="submit" class="btn btn-warning btn-lg ms-2 mb-5">Sign Up</button>
                  </div>
                  
                
                </form></div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
<div id="server-error" class="error"></div>
<div id="login-error" class="error"></div>
